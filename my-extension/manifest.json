{
    "manifest_version": 3,
    "name": "GIF Detection",
    "version": "1.0",
    "description": "This extension checks the gif for you",
    
    "icons": {
      "16": "icons/icon16.png",  
      "48": "icons/icon48.png",  
      "128": "icons/icon128.png"  
    },
    
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon16.png"
    },
  
    "permissions": [
      "activeTab"
    ]
  }
  